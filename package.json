{
  "name": "screenshot_backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:dev": "nest build && npm run sentry:sourcemaps-dev",
    "build": "nest build && npm run sentry:sourcemaps",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy:dev": "npm install && npm run build:dev && npm run start:prod",
    "deploy": "npm install && npm run build && npm run start:prod",
    "sentry:sourcemaps-dev": "sentry-cli sourcemaps inject --org off-0h --project aivee-backend-dev ./dist && sentry-cli --url https://sentry.io/ sourcemaps upload --org off-0h --project aivee-backend-dev ./dist",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org off-0h --project aivee-backend-prod ./dist && sentry-cli --url https://sentry.io/ sourcemaps upload --org off-0h --project aivee-backend-prod ./dist"
  },
